@model UniversiteOgrenciYonetimSistemi.Models.Ogrenci

<h2>Profile</h2>

@if (!string.IsNullOrEmpty(ViewBag.Message))
{
    <div class="alert alert-success">
        @ViewBag.Message
    </div>
}

<div>
    <p><strong>Ad:</strong> @Model.Ad</p>
    <p><strong>Soyad:</strong> @Model.Soyad</p>
    <p><strong>Üniversite:</strong> @Model.Universite</p>
    <p><strong>Bölüm:</strong> @Model.Bolum</p>
    <p><strong>Sınıf:</strong> @Model.Sinif</p>
    <p><strong>Email:</strong> @Model.Email</p>
</div>
